---
import LinkCards from "./LinkCards.astro";
import { Links } from "../utils/Links";
import type { LinkType } from "../utils/Links";
---

<ul class="text-justify m-5">
  {
    Links.map((link: LinkType) => {
      const { id, linkName } = link;

      return (
        <li>
          {id}. {linkName}
        </li>
      );
    })
  }
</ul>

<LinkCards />
